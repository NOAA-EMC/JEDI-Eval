#!/usr/bin/env python
import sys
from solo.configuration import Configuration
if len(sys.argv) < 2:
    print('expects a yaml configuration file name')
    exit(1)

config = Configuration(sys.argv[1])
if len(sys.argv) > 4:
    for key in sys.argv[3:]:
        print(f'{key}="{config[sys.argv[2]][key]}"')
else:
   key = sys.argv[2]
   print(f'{key}="{config[key]}"')
